{
  "name": "clawcontrol",
  "version": "0.12.0",
  "private": true,
  "scripts": {
    "dev": "cross-env DATABASE_URL=file:../data/clawcontrol.db node scripts/local-only.mjs next dev --webpack --hostname 127.0.0.1",
    "predev": "node scripts/ensure-next-postcss.mjs",
    "build": "cross-env DATABASE_URL=file:../data/clawcontrol.db next build --webpack",
    "prebuild": "node scripts/ensure-next-postcss.mjs && prisma generate",
    "start": "node scripts/local-only.mjs next start --hostname 127.0.0.1",
    "lint": "eslint .",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "pretypecheck": "prisma generate",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "templates:migrate-manager-role": "node scripts/migrate-manager-template-role.mjs",
    "stations:migrate-canonical": "cross-env DATABASE_URL=file:../data/clawcontrol.db node scripts/migrate-agent-stations-to-canonical.mjs",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@clawcontrol/adapters-openclaw": "*",
    "@clawcontrol/core": "*",
    "@clawcontrol/ui": "*",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/lang-markdown": "^6.5.0",
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@prisma/client": "^5.22.0",
    "@uiw/react-codemirror": "^4.25.4",
    "ajv": "^8.17.1",
    "clsx": "^2.1.0",
    "fast-uri": "^3.1.0",
    "geist": "^1.3.0",
    "js-yaml": "^4.1.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.468.0",
    "motion": "^12.29.2",
    "next": "^16.1.0",
    "prettier": "^3.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "shiki": "^3.21.0",
    "tailwind-merge": "^2.2.0",
    "use-stick-to-bottom": "^1.1.2",
    "ws": "^8.19.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/ws": "^8.18.1",
    "autoprefixer": "^10.4.17",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.0",
    "eslint-config-next": "^16.1.0",
    "postcss": "^8.4.35",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.0",
    "vitest": "^3.2.4"
  }
}
